resource "citrixadc_authenticationoauthaction" "${1:this}" {
    authorizationendpoint = "${2:authorizationendpoint}"
    clientid = "${3:clientid}"
    clientsecret = "${4:clientsecret}"
    name = "${5:name}"
    tokenendpoint = "${6:tokenendpoint}"
}