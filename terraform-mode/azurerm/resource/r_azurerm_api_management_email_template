resource "azurerm_api_management_email_template" "${1:this}" {
    api_management_name = "${2:api_management_name}"
    body = "${3:body}"
    resource_group_name = "${4:resource_group_name}"
    subject = "${5:subject}"
    template_name = "${6:template_name}"
}