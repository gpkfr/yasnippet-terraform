resource "azurerm_subscription_cost_management_export" "${1:this}" {
    name = "${2:name}"
    recurrence_period_end_date = "${3:recurrence_period_end_date}"
    recurrence_period_start_date = "${4:recurrence_period_start_date}"
    recurrence_type = "${5:recurrence_type}"
    subscription_id = "${6:subscription_id}"
}