resource "nutanix_foundation_ipmi_config" "${1:this}" {
    ipmi_gateway = "${2:ipmi_gateway}"
    ipmi_netmask = "${3:ipmi_netmask}"
    ipmi_password = "${4:ipmi_password}"
    ipmi_user = "${5:ipmi_user}"
}