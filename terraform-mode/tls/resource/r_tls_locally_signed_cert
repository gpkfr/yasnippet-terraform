resource "tls_locally_signed_cert" "${1:this}" {
    allowed_uses = "${2:allowed_uses}"
    ca_cert_pem = "${3:ca_cert_pem}"
    ca_private_key_pem = "${4:ca_private_key_pem}"
    cert_request_pem = "${5:cert_request_pem}"
    validity_period_hours = "${6:validity_period_hours}"
}